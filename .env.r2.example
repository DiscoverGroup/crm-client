# Cloudflare R2 Configuration
# Get these values from your Cloudflare R2 API Token

# Your Cloudflare Account ID
# Found in Cloudflare Dashboard > Account Settings
VITE_R2_ACCOUNT_ID=

# R2 API Token - Access Key ID
# Create in Cloudflare Dashboard > R2 > API Tokens
VITE_R2_ACCESS_KEY_ID=

# R2 API Token - Secret Access Key
VITE_R2_SECRET_ACCESS_KEY=

# Your R2 Bucket Name
VITE_R2_BUCKET_NAME=

# R2 Public URL - R2.dev subdomain or custom domain
# Get this from Cloudflare Dashboard > R2 > Your Bucket > Settings > Public Access
# Format: https://pub-<hash>.r2.dev
VITE_R2_PUBLIC_URL=
